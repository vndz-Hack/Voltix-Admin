local replicate_storage = game:service"ReplicatedStorage";
local players = game:service"Players";

local local_player = players.LocalPlayer;
local hit = replicate_storage.hit;

local tool = local_player:FindFirstChild("Stakeout", true);

if tool then
	print"got tool";

	for i = 1, 15 do
		hit:FireServer(players.vndz.Character.Head, tool);
	end

	print"dead";
end
